{
  "name": "wdmg/yii2-activity",
  "description": "User activity tracking system",
  "keywords": ["yii2", "yii2-activity", "activity", "tracking", "wdmg"],
  "type": "yii2-extension",
  "license": "MIT",
  "version": "1.2.0",
  "homepage": "https://github.com/wdmg/yii2-activity",
  "support": {
    "source": "https://github.com/wdmg/yii2-activity",
    "issues": "https://github.com/wdmg/yii2-activity/issues"
  },
  "authors": [
    {
      "name": "Alexsander Vyshnyvetskyy",
      "email": "alex.vyshnyvetskyy@gmail.com"
    },
    {
      "name": "W.D.M.Group, Ukraine",
      "email": "wdmg.com.ua@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "^2.0.40",
    "wdmg/yii2-base": "^1.2.4"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "wdmg\\activity\\": ""
    }
  },
  "extra": {
    "options": {
      "routePrefix": "admin",
      "surfingActivity": false,
      "backendSurfing": true,
      "frontendSurfing": false,
      "ignoringRoutes": [],
      "ignoringUsers": [],
      "ignoringIp": []
    }
  }
}